<Window x:Class="Proyecto_Cliente.Cliente.M_AsignarCalificacionesAlumnos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_Cliente.Cliente"
        mc:Ignorable="d"
        Title="M_AsignarCalificacionesAlumnos" Height="450" Width="800">
    <Grid Height="450" Width="800">
        <DataGrid
                        Name="dgEstudiantes"
                        Grid.Column="0"
                        AutoGenerateColumns="False"
                        IsReadOnly="True" Margin="48,55,256,72">
            <DataGrid.Columns>
                <DataGridTextColumn
                                Width="*"
                                Binding="{Binding nombre}"
                                Header="Nombre" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Click="Button_ClickRegresar" Margin="627,346,50,72">Regresar</Button>
        <Button Click="Button_ClickGuardar" Margin="627,195,50,223">Guardar</Button>
        <TextBox x:Name="tbCalificacion" Margin="644,104,66,296"></TextBox>
        
    </Grid>
</Window>
